package mytestpack;

import org.openqa.selenium.WebDriver;
//import org.openqa.selenium.firefox.FirefoxDriver;
import org.openqa.selenium.ie.InternetExplorerDriver;

public class mytestclass {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//Created object of webdriver
		//WebDriver driver = new FirefoxDriver();
		WebDriver driver = new InternetExplorerDriver();
		//To open URL
		driver.get("http://https://dcegsystem-test.nci.nih.gov/dcegcs/dashboard.action?changeUser=gagej#searchResults");
		
		//get current open URL
		String i = driver.getCurrentUrl();
		
		//Pring URL in console
		System.out.println(i);
		
		//Close firefox driver Instance 
		driver.close();
	}

}
